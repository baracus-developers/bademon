#. TXT_ASK_DHCP
#, c-format
msgid "Automatic configuration via %s?"
msgstr "Automaattinen määritys %s käyttäen?"

#. %s is either BOOTP or DHCP
#. TXT_SEND_DHCP
#, c-format
msgid "Sending %s request..."
msgstr "Lähetetään %s pyyntö..."

#. %s is either BOOTP or DHCP
#. TXT_ERROR_DHCP
#, c-format
msgid "%s configuration failed."
msgstr "%s määritys epäonnistui."

#. TXT_DHCP_VS_BOOTP
msgid "Use DHCP instead of BOOTP?"
msgstr "Käytä DHCP:tä BOOTP:n sijasta?"

#. TXT_MODDISK0
msgid "Insert a modules disk."
msgstr "Syötä moduulilevyke."

#. TXT_MODDISK1
#, c-format
msgid "Insert modules disk %d."
msgstr "Anna moduulilevyke %d."

#. TXT_MODDISK2
msgid "Find instructions for creating it in boot/README on CD1 or DVD1."
msgstr "Löydät ohjeet sen luomiseen boot/README CD1:ltä tai DVD1:ltä."

#. TXT_CHOOSE_NETSOURCE
msgid "Choose the network protocol."
msgstr "Valitse verkkoyhteyskäytäntö."

#. %s is NFS, FTP, HTTP or SMB
#. TXT_INPUT_NETSERVER
#, c-format
msgid "Enter the IP address of the %s server."
msgstr "Anna %s-palvelimen IP-osoite."

#. %s is FTP or HTTP
#. TXT_TRY_REACH_SERVER
#, c-format
msgid "Trying to connect to the %s server..."
msgstr "Yritetään yhdistää %s-palvelimeen..."

#. TXT_NET
msgid "Network"
msgstr "Verkko"

#. %s is e.g. SCSI, CD-ROM, USB
#. TXT_LOAD_MODULES
#, c-format
msgid "Load %s Modules"
msgstr "Lataa %s moduulit"

#. TXT_LOAD_FS
msgid "Load File System Modules"
msgstr "Lataa tiedostojärjestelmämoduulit"

#. TXT_LOAD_OTHER
msgid "Load Unknown Modules"
msgstr "Lataa tuntemattomat moduulit"

#. %s is e.g. ext3 or reiserfs
#. TXT_FILE_SYSTEM
#, c-format
msgid "%s file system found."
msgstr "%s tiedostojärjestelmä löytyi."

#. TXT_CONTINUE
msgid "Continue"
msgstr "Jatka"

#. TXT_ABORT
msgid "Abort"
msgstr "Keskeytä"

