#. TXT_ASK_DHCP
#, c-format
msgid "Automatic configuration via %s?"
msgstr "Chcete automaticky konfigurovat přes %s?"

#. %s is either BOOTP or DHCP
#. TXT_SEND_DHCP
#, c-format
msgid "Sending %s request..."
msgstr "Odesílám %s požadavek..."

#. %s is either BOOTP or DHCP
#. TXT_ERROR_DHCP
#, c-format
msgid "%s configuration failed."
msgstr "Konfigurace přes %s se nepovedla."

#. TXT_DHCP_VS_BOOTP
msgid "Use DHCP instead of BOOTP?"
msgstr "Použít DHCP místo BOOTP?"

#. TXT_MODDISK0
msgid "Insert a modules disk."
msgstr "Vložte disk s moduly."

#. TXT_MODDISK1
#, c-format
msgid "Insert modules disk %d."
msgstr "Vložte disk s moduly číslo %d."

#. TXT_MODDISK2
msgid "Find instructions for creating it in boot/README on CD1 or DVD1."
msgstr "Návod na vytvoření naleznete na CD1 nebo DVD1 v adresáři boot/README."

#. TXT_CHOOSE_NETSOURCE
msgid "Choose the network protocol."
msgstr "Zvolte síťový protokol."

#. %s is NFS, FTP, HTTP or SMB
#. TXT_INPUT_NETSERVER
#, c-format
msgid "Enter the IP address of the %s server."
msgstr "Zadejte IP adresu %s serveru."

#. %s is FTP or HTTP
#. TXT_TRY_REACH_SERVER
#, c-format
msgid "Trying to connect to the %s server..."
msgstr "Pokouším se připojit %s server..."

#. TXT_NET
msgid "Network"
msgstr "Síť"

#. %s is e.g. SCSI, CD-ROM, USB
#. TXT_LOAD_MODULES
#, c-format
msgid "Load %s Modules"
msgstr "Nahrát %s moduly"

#. TXT_LOAD_FS
msgid "Load File System Modules"
msgstr "Nahrát moduly souborového systému"

#. TXT_LOAD_OTHER
msgid "Load Unknown Modules"
msgstr "Nahrát neznámé moduly"

#. %s is e.g. ext3 or reiserfs
