#, c-format
msgid "%s file system found."
msgstr "Souborový systém %s byl nalezen."

#. TXT_CONTINUE
msgid "Continue"
msgstr "Pokračovat"

#. TXT_ABORT
msgid "Abort"
msgstr "Přerušit"

#. TXT_NO_NEW_MODULES
msgid "No new modules found."
msgstr "Nebyly nalezeny nové moduly."

#. TXT_CHOOSE_PARTITION
msgid "Choose the hard disk partition."
msgstr "Zvolte diskový oddíl."

#. TXT_CHOOSE_ROOT_FS
msgid "Choose the root partition."
msgstr "Zvolte kořenový (root) oddíl."

#. TXT_CHOOSE_SWAP
msgid "Choose the swap partition."
msgstr "Zvolte odkladový (swap) oddíl."

#. TXT_LOW_MEMORY1
msgid "There is not enough memory to load all data."
msgstr "Nemáte dostatek paměti pro nahrání všech dat."

#. TXT_LOW_MEMORY2
msgid "Your computer does not have enough memory to run YaST."
msgstr "Počítač nemá dostatek operační paměti pro spuštění programu YaSTu."

