#, c-format
msgid "%s file system found."
msgstr "ifayela %s yesistimu etholakele."

#. TXT_CONTINUE
msgid "Continue"
msgstr "Qhubeka"

#. TXT_ABORT
msgid "Abort"
msgstr "Yeka"

#. TXT_NO_NEW_MODULES
msgid "No new modules found."
msgstr "Awekho amamoduli amasha atholakele."

#. TXT_CHOOSE_PARTITION
msgid "Choose the hard disk partition."
msgstr "Khetha ikamelo likagalaza (hard disk)."

#. TXT_CHOOSE_ROOT_FS
msgid "Choose the root partition."
msgstr "Khetha ikamelo lomqondisi (root)"

#. TXT_CHOOSE_SWAP
