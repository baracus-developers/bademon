msgid "Devices"
msgstr "Zariadenia"

#. TXT_INFO_NETDEV
msgid "Network Cards"
msgstr "Sieťové karty"

#. TXT_INFO_DMA
msgid "DMA"
msgstr "DMA"

#. TXT_INFO_IRQS
msgid "Interrupts"
msgstr "Prerušenia"

#. TXT_INFO_IOPORTS
msgid "I/O Ports"
msgstr "I/O Porty"

